      <name>catalog001</name>
      <creator origin="Saxonica">Michael Kay</creator>
      <category>Schema</category>
      <description>Test that the test catalog is valid with respect to its schema</description>
      <section number="0" spec="XSLT20" version="2005-11-03"/>
      <discretionary-items>
         <discretionary-feature name="schema_aware" behavior="on"/>
      </discretionary-items>
      <input>
         <stylesheet file="verify-catalog.xsl" role="principal"/>
         <source-document file="../catalog.xml" role="principal"/>
         <schema role="source-reference" file="../catalog.xsd" validates="catalog.xml"/>
      </input>
      <output same-as-1.0="false">
         <result-document file="ok.xml" role="principal" type="xml"/>
      </output>
   </testcase>

