   <description>Tests result-document</description>
   <environment name="result-document-01">
      <source role=".">
         <content><![CDATA[ 
<doc>
<foo place="secondary">Hello</foo>
</doc>]]></content>
      </source>
   </environment>
   <environment name="result-document-02">
      <source role=".">
         <content><![CDATA[<!-- A required but useless input XML doc for a transformation -->
<doc/>]]></content>
      </source>
   </environment>
   <environment name="result-document-03">
      <source role=".">
         <content><![CDATA[<doc>
	<foo>text</foo>
</doc>]]></content>
      </source>
   </environment>
   <environment name="result-document-04">
      <source role=".">
         <content><![CDATA[<doc>
	<foo>item1</foo>
	<foo>item</foo>
	<elem>item3</elem>
	<item>my:item5</item>
</doc>]]></content>
      </source>
   </environment>
   <environment name="result-document-05">
      <source role=".">
         <content><![CDATA[<doc>
	<foo>out.dtd</foo>
</doc>]]></content>
      </source>
   </environment>
   <environment name="result-document-06">
      <source role=".">
         <content><![CDATA[<doc>
	<foo>-//BOAG//DTD Websites V1.3//EN</foo>
</doc>]]></content>
      </source>
   </environment>
   <environment name="result-document-07">
      <source role=".">
         <content><![CDATA[<doc>
	<foo>yes</foo>
</doc>]]></content>
      </source>
   </environment>
   <environment name="result-document-08">
      <source role=".">
         <content><![CDATA[<doc>
	<foo>1.0</foo>
</doc>]]></content>
      </source>
   </environment>
   <environment name="result-document-09">
      <source role=".">
         <content><![CDATA[<doc>
	<foo>one:</foo>
</doc>]]></content>
      </source>
   </environment>
   <environment name="result-document-10">
      <source role=".">
         <content><![CDATA[<doc/>]]></content>
      </source>
   </environment>

   <test-case name="result-document-0101">
      <description>Switch to secondary output and back to primary - sequentially.</description>
      <created by="Michael Kay" on="2012-10-30"/>
      <environment ref="result-document-01"/>
      <dependencies>
         <spec value="XSLT20+"/>
      </dependencies>
      <test>
         <stylesheet file="result-document-0101.xsl"/>
      </test>
      <result>
         <all-of>
            <assert-xml><![CDATA[<out><log>Before redirect</log><log>After redirect</log></out>]]></assert-xml>
            <assert-result-document uri="multresult1.out">
               <assert-xml file="result-document-0101-2.out"/>
            </assert-result-document>
         </all-of>
      </result>
   </test-case>

